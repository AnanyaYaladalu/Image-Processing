    "import cv2\n",
    "from PIL import Image\n",
    "img1 = Image.open(\"back.jpg\")\n",
    "img2 = Image.open(\"fore.png\")\n",
    "\n",
    "img1.paste(img2,(0,0),mask=img2)\n",
    "img1.show()\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
